{
  "name": "xgbnl/validator",
  "description": "laravel validator,support scene validate.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Koala\\Validation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "App\\": "tests/Unit/App/"
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": "^8.2",
    "xgbnl/guesser": "dev-main"
  },
  "require-dev": {
    "illuminate/validation": "^10.15",
    "illuminate/support": "^10.15",
    "illuminate/http": "^10.15",
    "laravel/framework": "^10.15",
    "xgbnl/dingo": "dev-main",
    "phpunit/phpunit": "^10.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Koala\\Validation\\ValidationServiceProvider"
      ]
    }
  }
}
